function GetOpcodeName(iOp: Integer): String;
procedure GenerateRawInfo(iCmd: Integer; sBody: String; bFromServer: Boolean;
  cLogger: TSchGenericLogger);
procedure GenerateFormattedInfo(iCmd: Integer; sBody: String;
  bFromServer: Boolean; cLogger: TSchGenericLogger);
procedure GenerateBodyInfo(sBody: String; cLogger: TSchGenericLogger);

implementation

uses Debug, Globals;

function GetOpcodeName(iOp: Integer): String;
var
  i: Integer;
begin

  for i := 1 to MAX_OPCODES do if OpcodeToString[i].wMsg = iOp then
    begin
      Result := OpcodeToString[i].sName;

      Exit;
    end;

  Result := 'Unknown';
end;

function ReConst(Ch: Char): Char;
begin
  if Ch in [#32..#127] then Result := Ch
  else
    Result := '.';
end;

procedure GenerateBodyInfo(sBody: String; cLogger: TSchGenericLogger);
var
  i: Integer;
  sr, sd: String;
  Ch: Char;
begin
  cLogger.Log('|------------------------------------------------|----------------|',
    True);
  cLogger.Log('|00 01 02 03 04 05 06 07 08 09 0A 0B 0C 0D 0E 0F |0123456789ABCDEF|',
    True);
  cLogger.Log('|------------------------------------------------|----------------|',
    True);

  i := 0;
  sr := '';
  sd := '';
  while Length(sBody) > 0 do
  begin
    Ch := sBody[1];
    Delete(sBody, 1, 1);

    sr := sr + IntToHex(Byte(Ch), 2) + ' ';
    sd := sd + ReConst(Ch);

    Inc(i);

    if i > $0f then
    begin
      cLogger.Log('|' + sr + '|' + sd + '|', True);
      sr := '';
      sd := '';
      i := 0;
    end;
  end;

  if sr <> '' then
  begin
    while i <= $0f do
    begin
      sr := sr + '   ';
      sd := sd + ' ';
      Inc(i);
    end;
    cLogger.Log('|' + sr + '|' + sd + '|', True);
  end;

  cLogger.Log('-------------------------------------------------------------------',
    True);
  cLogger.NewLine();
end;


procedure GenerateFormattedInfo(iCmd: Integer; sBody: String;
  bFromServer: Boolean; cLogger: TSchGenericLogger);
var
  sFrom: String;

begin

  if bFromServer then sFrom := '{SERVER}'
  else
    sFrom := '{CLIENT}';

  cLogger.Log(sFrom, lcBlue, [fsBold]);
  cLogger.Log(' Packet: (0x' + IntToHex(iCmd, 4) + ') ', lcBlack, [fsBold]);
  cLogger.Log(GetOpcodeName(iCmd), lcGreen, [fsBold]);
  cLogger.Log(' Size = ' + IntToStr(Length(sBody)), lcBlack, [fsBold]);
  cLogger.NewLine();

  GenerateBodyInfo(sBody, cLogger);
end;

procedure GenerateRawInfo(iCmd: Integer; sBody: String; bFromServer: Boolean;
  cLogger: TSchGenericLogger);
begin
  cLogger.Log(IntToStr(Integer(bFromServer)) + '|' + IntToStr(
    iCmd) + '|' + ToHexRep(sBody), True);
end;

end.
